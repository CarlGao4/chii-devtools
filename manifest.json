{
    "manifest_version": 3,
    "type": "extension",
    "name": "Chii DevTools",
    "slug": "chii_devtools",
    "description": "使用Chii的DevTools进行远程调试",
    "version": "0.1.0",
    "author": {
        "name": "沫烬染",
        "link": "https://github.com/mo-jinran"
    },
    "repository": {
        "repo": "mo-jinran/chii-devtools",
        "branch": "v3",
        "use_release": {
            "tag": "0.1.0",
            "name": "chii-devtools.zip"
        }
    },
    "platform": [
        "win32"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}
